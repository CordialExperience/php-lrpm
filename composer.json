{
    "name": "php-lrpm/php-lrpm",
    "description": "PHP Long Running Process Manager",
    "type": "library",
    "keywords": ["php", "long-running", "process", "manager", "posix"],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/vrza/array-with-secondary-keys"
        },
        {
            "type": "vcs",
            "url": "https://github.com/vrza/cardinal-collections"
        },
        {
            "type": "vcs",
            "url": "https://github.com/vrza/php-tipc"
        },
        {
            "type": "vcs",
            "url": "https://github.com/vrza/php-pretty-print"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "vrza/array-with-secondary-keys": "dev-main",
        "vrza/php-tipc": "dev-main",
        "vrza/php-pretty-print": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "PHPLRPM\\": "src/",
            "PHPLRPM\\Test\\": "test/"
        }
    }
}
